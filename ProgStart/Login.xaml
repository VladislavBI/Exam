        Title="Login" Height="156" Width="292">
    <Grid>
        <Label Content="Логин" HorizontalAlignment="Left" Margin="17,25,0,0" VerticalAlignment="Top"/>
        <Label Content="Пароль" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="UsernameTBox" HorizontalAlignment="Left" Height="23" Margin="86,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordTBox" HorizontalAlignment="Left" Height="23" Margin="86,59,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="ProgStartButton" Content="Запуск" HorizontalAlignment="Left" Margin="10,87,0,0" VerticalAlignment="Top" Width="75" Click="ProgStartButton_Click"/>

    </Grid>
</Window>
